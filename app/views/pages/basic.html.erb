<%= render 'partials/navbar' %>
<div class="page-container">
	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<h2 class="latoblack question-hdr">Basic ID Divorce <span class="pull-right">$1,500</h2>
				<p class="latolight main-text">Fill out the form below to get started. If you have property or a business that is jointly owned you will need to use the <a href="#">ID with Property</a> form. If you have children you will alternatively need to fill out the <a href="#">ID with Children</a> form.</p>
			</div>
		</div>
		<%= form_for Contact.new do |f| %>
		<div class="row">
			<div class="col-md-6">
				<div class="form-group">
					<label class="sr-only">Full Name</label>
					<%= f.text_field :name, class: "form-control", placeholder: "Full Name" %>
				</div>
			</div>
		</div>
		<% end %>
	</div>
</div>
<%= render 'partials/footer' %>