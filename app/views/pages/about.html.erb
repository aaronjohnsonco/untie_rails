            </div>
            <div class="col-md-6">
              <div class="form-group">
                <label class="green-label latoregular">State</label>
                <%= f.text_field :state, class: "form-control-modal" %>
              </div>
            </div>
            <div class="col-md-6">
              <div class="form-group">
                <label class="green-label latoregular">Zip</label>
                <%= f.text_field :zip, class: "form-control-modal" %>
              </div>
            </div>

            <div class="col-md-6">
              <div class="form-group">
                <label class="green-label latoregular">How long resided there?</label>
                <%= f.text_field :resided_how_long, class: "form-control-modal" %>
              </div>
            </div>
            <div class="col-md-6">
              <div class="form-group">
                <label class="green-label latoregular">Email</label>
                <%= f.text_field :email, class: "form-control-modal" %>
              </div>
            </div>
            <div class="col-md-6">
              <div class="form-group">
                <label class="green-label latoregular">Phone</label>
                <%= f.text_field :phone, class: "form-control-modal" %>
              </div>
            </div>
            <div class="col-md-6">
              <div class="form-group">
                <label class="green-label latoregular">How many children?</label>
                <%= f.text_field :number_of_children, class: "form-control-modal" %>
              </div>
            </div>
            <div class="col-md-6">
              <div class="form-group">
                <label class="green-label latoregular">What are their ages?</label>
                <%= f.text_field :ages, class: "form-control-modal" %>
              </div>
            </div>
            <div class="col-md-6">
              <div class="checkbox">
                <label class="green-label latoregular">
                  <%= f.check_box :co_own_business %>
                  Check if you co-own a business
                </label>
              </div>
            </div>
            <div class="col-md-6">
              <div class="checkbox">
                
                <label class="green-label latoregular">
                  <%= f.check_box :co_own_property %>
                  Check if you co-own real estate
                </label>
              </div>
            </div>
          </div>
        
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <%= f.submit "Submit", class: "btn btn-bright", name: "about3" %>
      </div>
      <% end %>
    </div>
  </div>
</div>
